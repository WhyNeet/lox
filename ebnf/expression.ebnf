expression = equality ;

(* Based on precedence level (from lowest to highest) *)
equality = comparison { ( "!=" | "==" ) comparison } ;
comparison = term { ( ">" | ">=" | "<" | "<=" ) term } ;
term  = factor { ( "+" | "-" ) factor } ;
factor = factor ( "/" | "*" ) unary | unary ;
unary = ( "-" | "!" ) unary | primary ;
primary = NUMBER | STRING | "true" | "false" | "nil" | "(" expression ")" ;
